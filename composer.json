{
    "name": "alpinegreen/kernel",
    "type": "library",
    "description": "A lightweight PHP application kernel based on Symfony dependency injection container",
    "keywords": [],
    "homepage": "https://alpinegreen.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Bohan Yang",
            "email": "youthdna@live.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/config": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "AlpineGreen\\Kernel\\": "src/"
        }
    },
    "require-dev": {
        "doctrine/coding-standard": "^9.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
